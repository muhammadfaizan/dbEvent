    <script src="javascripts/noti.js"></script>

    <style>
        .nav, .pagination, .carousel, .panel-title a { cursor: pointer; }
    </style>
    <script>
        $("#NotiPanel").on("click", function () {
            ShowCrystalNotificationPanel()
        });
