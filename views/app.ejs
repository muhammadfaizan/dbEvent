                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">dbevent</a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav">
                <li class="active"><a href="#">Home</a></li>

            </ul>
            <ul class="nav navbar-nav navbar-right">

                <li id="noti"><a><span id="NotiPanel" class="badge"><span style="color: #ffffff; padding-right: 10px;" class="glyphicon glyphicon-bullhorn"></span>{{totalNotification}}</span></a></li>
                <li>
                    <form class="navbar-form" role="form"method="get" action="/logout">
                        <button class="btn btn-danger" type="submit">Log out</button>
                    </form>
                </li>

            </ul>

        </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
</nav>
<div ng-view>

</div>

